CREATE TABLE IF NOT EXISTS Tile (
    Zoom INTEGER AS kotlin.Int NOT NULL,
    X INTEGER AS kotlin.Int NOT NULL,
    Y INTEGER AS kotlin.Int NOT NULL,
    Width REAL NOT NULL,
    Height REAL NOT NULL,
    Content BLOB NOT NULL,
    PRIMARY KEY (Zoom, X, Y)
);

getTileById:
SELECT * FROM Tile
WHERE Zoom = ?
AND X = ?
AND Y = ?;

deleteTiles:
DELETE FROM Tile;