import com.bselzer.gw2.v2.model.enumeration.wrapper.WorldName;
import com.bselzer.gw2.v2.model.world.WorldId;

CREATE TABLE IF NOT EXISTS World (
    Id INTEGER AS WorldId NOT NULL,
    Name TEXT AS WorldName NOT NULL,
    PRIMARY KEY (Id)
);

getAll:
SELECT * FROM World;

insertOrReplace:
REPLACE INTO World(
    Id,
    Name
) VALUES :world;

deleteAll:
DELETE FROM World;