import com.bselzer.gw2.v2.model.wvw.match.WvwMatch;
import com.bselzer.gw2.v2.model.wvw.match.WvwMatchId;

CREATE TABLE IF NOT EXISTS WvwMatch (
    Id TEXT AS WvwMatchId NOT NULL,
    Model TEXT AS WvwMatch NOT NULL,
    PRIMARY KEY (Id)
);

getAll:
SELECT * FROM WvwMatch;

insertOrReplace:
REPLACE INTO WvwMatch (
    Id,
    Model
) VALUES :wvwMatch;

deleteAll:
DELETE FROM WvwMatch;