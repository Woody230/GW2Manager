import com.bselzer.gw2.v2.model.wvw.upgrade.WvwUpgrade;
import com.bselzer.gw2.v2.model.wvw.upgrade.WvwUpgradeId;

CREATE TABLE IF NOT EXISTS WvwUpgrade (
    Id INTEGER AS WvwUpgradeId NOT NULL,
    Model TEXT AS WvwUpgrade NOT NULL,
    PRIMARY KEY (Id)
);

getAll:
SELECT * FROM WvwUpgrade;

insertOrReplace:
REPLACE INTO WvwUpgrade(
    Id,
    Model
) VALUES :wvwUpgrade;

deleteAll:
DELETE FROM WvwUpgrade;